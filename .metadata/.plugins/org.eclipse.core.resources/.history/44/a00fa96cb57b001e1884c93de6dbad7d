package data.vo;

public class Education_Reasult_Score_VO {
	
	String category_name;
	float category_everage; //학생 점수
	int evaluation_grade; // 교수 점수
	public String getCategory_name() {
		return category_name;
	}
	
	public void setCategory_name(String category_name) {
		this.category_name = category_name;
	}
	
	public float getCategory_everage() {
		return category_everage;
	}
	
	public void setCategory_everage(float category_everage) {
		this.category_everage = category_everage;
	}
	
	public int getEvaluation_grade() {
		
		return evaluation_grade;
	}
	
	public void setEvaluation_grade(int evaluation_grade) {
		this.evaluation_grade = evaluation_grade;
	}
	
	public Education_Reasult_Score_VO(String category_name, float category_everage, int evaluation_grade) {
		super();
		this.category_name = category_name;
		this.category_everage = category_everage;
		this.evaluation_grade = evaluation_grade;
	}
	
	public Education_Reasult_Score_VO() {
		super();
	}
	
	@Override
	public String toString() {
		return "Education_Reasult_VO [category_name=" + category_name + ", category_everage=" + category_everage
				+ ", evaluation_grade=" + evaluation_grade + "]";
	}
	
	

}
